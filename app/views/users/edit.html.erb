<%= form_for @user do |f| %>
  <%= f.label :username, "Your username"%>
  <%= f.text_field :username %>
  <br>
  <%= f.label :email, "Your email" %>
  <%= f.email_field :email%>
  <br>
  <%= f.label :password, "Your password" %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit "Submit" %>
<% end %>
